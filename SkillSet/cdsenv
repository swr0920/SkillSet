    ;;Display none-grid
    layout drawDottedGridOn boolean t
    layout drawGridOn boolean nil
    layout dotStyleMajorGrid boolean nil

    ;;Windows Position Setting
    layoutXL ciwWindow string "((0.0 1081.0)(1920.0 1720.0))"
